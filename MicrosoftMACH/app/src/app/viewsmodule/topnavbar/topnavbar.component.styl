.commit-modal
    // position absolute
    top 20%
    // left 50%
    // -ms-transform transalate(-50%,-50%)
    // -o-transform transalate(-50%,-50%)
    // -webkit-transform transalate(-50%,-50%)
    // transform transalate(-50%,-50%)
    .modal-content
        border-radius 0
    .button-container
        padding 0 15px 10px
        text-align center
        .btn
            cursor pointer
            border-radius 0
            color #FFF
            calc-outlet
                color #FFF
        .yes-button
            border 1px solid $er-color1
            background-color $er-color1
            // float left
        .no-button
            border 1px solid $er-color4
            background-color $er-color4
            // float right
        &:after
