{
  "name": "autodesk-ms2",
  "version": "1.0.0",
  "description": "WIB Isomer using angular2 and webpack",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "build": "npm run build:dev",
    "build:dev": "node --max_old_space_size=7096 node_modules/webpack/bin/webpack.js --config config/webpack.dev.js --progress --profile",
    "build:prod": "node --max_old_space_size=7096 node_modules/webpack/bin/webpack.js --config config/webpack.prod.js  --progress --profile --bail",
    "webpack-dev-server": "node --max_old_space_size=7096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "webpack",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist --cors",
    "extract": "ng-xi18n",
    "build:deploy": "npm run build:prod && grunt build"
  },
  "license": "UNLICENSED",
